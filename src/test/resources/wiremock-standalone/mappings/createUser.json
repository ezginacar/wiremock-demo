{
  "request": {
    "method": "POST",
    "urlPath": "/create",
    "bodyPatterns": [
    {
      "matchesJsonPath": "$.name"
    },
    {
      "matchesJsonPath": "$.salary"
    },
    {
      "matchesJsonPath": "$.age"
    }
    ]

  },
  "response": {
    "status": 200,
    "body": {
      "status": "success",
      "data": {
        "name": "$(name)",
        "salary": "$(salary)",
        "age": "$(age)",
        "id": -1,
        "message": "Successfully! Record has been added."
      },

      "transformers": [
        "body-transformer"
      ]

    }
  }
}


